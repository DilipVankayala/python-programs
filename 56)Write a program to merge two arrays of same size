def merge_arrays(arr1, arr2):
    merged_array = arr1 + arr2
    return merged_array
if __name__ == "__main__":
    array1 = [1, 2, 3, 4, 5]
    array2 = [6, 7, 8, 9, 10]
    merged = merge_arrays(array1, array2)
    print("Array 1:", array1)
    print("Array 2:", array2)
    print("Merged Array:", merged)
